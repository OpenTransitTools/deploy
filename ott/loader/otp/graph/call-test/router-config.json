{
  "routingDefaults": {
    "maxJourneyDuration": "6h",
    "maxHours": 6,
    "useRequestedDateTimeInMaxHours": true,
    "boardSlack": "0M",
    "alightSlack": "0M",
    "transferSlack": 180,
    "maxTransfers": 4,
    "waitReluctance": 0.9,
    "walkReluctance": 2.75,
    "stairsReluctance": 1.65,
    "carReluctance": 16,
    "numItineraries": 3,
    "disableAlertFiltering": true,
    "invalidDateStrategy": "USE_CURRENT",
    "nonTransitGeneralizedCostLimit": "0 + 0.5x",
    "itineraryFilters": {
      // only show non-transit (ie. walking) when it's at least as good as the transit option
      "nonTransitGeneralizedCostLimit": "0 + 1.0 x"
    }
  },
  "transit": {
    "maxNumberOfTransfers": 4,
    "dynamicSearchWindow": {
      "minTransitTimeCoefficient": 0.5,
      "minWaitTimeCoefficient": 0.5,
      "minWindow": "8h",
      "maxWindow": "8h"
    }
  },
  "updaters": [
    {
      "type": "real-time-alerts",
      "frequency": 20,
      "url": "http://trimet.org/transweb/ws/V1/FeedSpecAlerts/includeFuture/true/suppressSystemWideAlerts/true/",
      "feedId": "TriMet"
    },
    {
      "type": "stop-time-updater",
      "frequency": 31,
      "url": "http://trimet.org/transweb/ws/V1/TripUpdate",
      "feedId": "TriMet"
    },
    {
      "type": "vehicle-positions",
      "frequency": 37,
      "url": "http://trimet.org/transweb/ws/V1/VehiclePositions",
      "feedId": "TriMet"
    },
    {
      "type": "vehicle-rental",
      "frequency": 40,
      "sourceType": "gbfs",
      "url": "https://gbfs.biketownpdx.com/gbfs/2.3/gbfs.json"
    },
    {
      "type": "vehicle-rental",
      "frequency": 41,
      "network": "BIRD",
      "sourceType": "gbfs",
      "url": "https://mds.bird.co/gbfs/v2/public/portland/gbfs.json",
      "geofencingZones": "true"
    },
    {
      "type": "vehicle-rental",
      "frequency": 43,
      "network": "LIME",
      "sourceType": "gbfs",
      "url": "https://data.lime.bike/api/partners/v2/gbfs/portland/gbfs.json"
    },
    {
      "type": "vehicle-rental",
      "frequency": 45,
      "network": "SPIN",
      "sourceType": "gbfs", 
      "url": "https://gbfs.spin.pm/api/gbfs/v2_3/portland/gbfs",
      "geofencingZones": "true"
    }
  ],
 "vectorTileLayers": [
    {
      "name": "stops",
      "type": "Stop",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 14,
      "cacheMaxSeconds": 600
    },
    {
      "name": "stations",
      "type": "Station",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 2,
      "cacheMaxSeconds": 600
    },
    {
       "name": "rentalVehicles",
       "type": "VehicleRentalVehicle",
       "mapper": "Digitransit",
       "maxZoom": 20,
       "minZoom": 2,
       "cacheMaxSeconds": 60
     },
     {
       "name": "rentalStations",
       "type": "VehicleRentalStation",
       "mapper": "Digitransit",
       "maxZoom": 20,
       "minZoom": 2,
       "cacheMaxSeconds": 600
     },
    {
      "name": "vehicleParking",
      "type": "VehicleParking",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 10,
      "cacheMaxSeconds": 60,
      "expansionFactor": 0.25
    }
  ],
  "rideHailingServices": [
    {
      "type": "uber-car-hailing",
      "clientId": "${UBER_CLIENT_ID}",
      "clientSecret": "${UBER_CLIENT_SECRET}",
      "wheelchairAccessibleProductId": "970ed376-bd33-4ad1-81c5-d2928a347a06"
    }
  ]
}
