{
  "routingDefaults": {
    "maxJourneyDuration": "6h",
    "boardSlack": "0M",
    "alightSlack": "0M",
    "transferSlack": 180,
    "waitReluctance": 0.9,
    "walkReluctance": 2.75,
    "stairsReluctance": 1.65,
    "carReluctance": 16,
    "numItineraries": 3,
    "itineraryFilters": {
      // only show non-transit (ie. walking) when it's at least as good as the transit option
      "nonTransitGeneralizedCostLimit": "0 + 1.0 x"
    }
  },
  "transit": {
    "maxNumberOfTransfers": 4,
    "dynamicSearchWindow": {
      "minTransitTimeCoefficient": 0.5,
      "minWaitTimeCoefficient": 0.5,
      "minWindow": "8h",
      "maxWindow": "8h"
    }
  },
  "flex": {
    "maxAccessWalkDuration": "10m",
    "maxEgressWalkDuration": "10m"
  },
  "updaters": [
    {
      "type": "stop-time-updater",
      "frequency": "31s",
      "url": "http://gtfs-rt.c-tran.org/gtfsrealtime/TripUpdates",
      "feedId": "CTRAN"
    },
    {
      "type": "vehicle-positions",
      "frequency": "29s",
      "url": "http://gtfs-rt.c-tran.org/gtfsrealtime/VehiclePositions",
      "feedId": "CTRAN"
    },
    {
      "type": "real-time-alerts",
      "frequency": "45s",
      "url": "https://mysmartbus.com/gtfs-rt/alerts",
      "feedId": "SMART"
    },
    {
      "type": "stop-time-updater",
      "frequency": "31s",
      "url": "https://mysmartbus.com/gtfs-rt/tripupdates",
      "feedId": "SMART"
    },
    {
      "type": "vehicle-positions",
      "frequency": "29s",
      "url": "https://mysmartbus.com/gtfs-rt/vehiclepositions",
      "feedId": "SMART"
    },
    {
      "type": "real-time-alerts",
      "frequency": "45s",
      "url": "http://trimet.org/transweb/ws/V1/FeedSpecAlerts/includeFuture/true/suppressSystemWideAlerts/true",
      "feedId": "TRIMET"
    },
    {
      "type": "stop-time-updater",
      "frequency": "31s",
      "url": "http://trimet.org/transweb/ws/V1/TripUpdate",
      "feedId": "TRIMET"
    },
    {
      "type": "vehicle-positions",
      "frequency": "29s",
      "url": "http://trimet.org/transweb/ws/V1/VehiclePositions",
      "feedId": "TRIMET"
    },
    {
      "type": "vehicle-positions",
      "frequency": "29s",
      "url": "https://ewp.transloc.com/subscriptions/gtfsrt/vehicles.ashx",
      "feedId": "WAPARK"
    },
    {
      "type": "real-time-alerts",
      "frequency": "45s",
      "url": "https://ycta.connexionz.net/rtt/public/utility/gtfsrealtime.aspx/alert",
      "feedId": "YAMHILL"
    },
    {
      "type": "stop-time-updater",
      "frequency": "31s",
      "url": "https://ycta.connexionz.net/rtt/public/utility/gtfsrealtime.aspx/tripupdate",
      "feedId": "YAMHILL"
    },
    {
      "type": "vehicle-positions",
      "frequency": "29s",
      "url": "https://ycta.connexionz.net/rtt/public/utility/gtfsrealtime.aspx/vehicleposition",
      "feedId": "YAMHILL"
    },
    {
      "type": "vehicle-rental",
      "frequency": "40s",
      "sourceType": "gbfs",
      "url": "https://gbfs.biketownpdx.com/gbfs/2.3/gbfs.json"
    },
    {
      "type": "vehicle-rental",
      "frequency": "43s",
      "network": "LIME",
      "sourceType": "gbfs",
      "url": "https://data.lime.bike/api/partners/v2/gbfs/portland/gbfs.json"
    },
    {
      "type": "vehicle-rental",
      "frequency": "45s",
      "network": "SPIN",
      "sourceType": "gbfs", 
      "url": "https://gbfs.spin.pm/api/gbfs/v2_3/portland/gbfs",
      "geofencingZones": "true"
    }
  ],
  "vectorTiles": {
    "basePath": "/rtp/routers/default/vectorTiles",
    "attribution": "<a href='https://trimet.org/mod'>Regional Partners</a>",
    "layers": [
      {
        "name": "stops",
        "type": "Stop",
        "mapper": "Digitransit",
        "maxZoom": 20,
        "minZoom": 14,
        "cacheMaxSeconds": 600
      },
      {
        "name": "stations",
        "type": "Station",
        "mapper": "Digitransit",
        "maxZoom": 20,
        "minZoom": 2,
        "cacheMaxSeconds": 600
      },
      {
        "name": "rentalVehicles",
        "type": "VehicleRentalVehicle",
        "mapper": "Digitransit",
        "maxZoom": 20,
        "minZoom": 2,
        "cacheMaxSeconds": 60
      },
      {
        "name": "rentalStations",
        "type": "VehicleRentalStation",
        "mapper": "Digitransit",
        "maxZoom": 20,
        "minZoom": 2,
        "cacheMaxSeconds": 600
      },
      {
        "name": "vehicleParking",
        "type": "VehicleParking",
        "mapper": "Digitransit",
        "maxZoom": 20,
        "minZoom": 10,
        "cacheMaxSeconds": 60,
        "expansionFactor": 0.25
      }
    ]
  },
  "rideHailingServices": [
    {
      "type": "uber-car-hailing",
      "clientId": "${UBER_CLIENT_ID}",
      "clientSecret": "${UBER_CLIENT_SECRET}",
      "wheelchairAccessibleProductId": "970ed376-bd33-4ad1-81c5-d2928a347a06"
    }
  ]
}
